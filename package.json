  "description": "Inquirer file selector prompt.",
  "version": "1.0.1",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["./dist"],
  "scripts": {
    "prepare": "husky",
