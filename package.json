  "devDependencies": {
    "@biomejs/biome": "2.3.6",
    "@rollup/plugin-typescript": "12.3.0",
    "husky": "9.1.7",
    "nano-staged": "0.9.0",
    "rimraf": "6.1.2",
    "rollup": "4.53.3",
    "rollup-plugin-dts": "6.2.3",
    "rollup-plugin-node-externals": "8.1.2",
    "tslib": "2.8.1",
    "typescript": "5.6.3"
  },
  "peerDependencies": {
    "@types/node": ">=20"
  },
  "peerDependenciesMeta": {
    "@types/node": {
