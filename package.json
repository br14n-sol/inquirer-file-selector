  "description": "Inquerer file selector prompt.",
  "version": "0.6.1",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["./dist"],
  "scripts": {
