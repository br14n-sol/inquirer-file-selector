    "@commitlint/config-conventional": "19.6.0",
    "@inquirer/select": "4.0.2",
    "@inquirer/type": "3.0.1",
    "@tsconfig/node18": "18.2.4",
    "husky": "9.1.7",
    "lint-staged": "15.2.10",
    "tsup": "8.3.5",
    "typescript": "5.6.3"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/br14n-sol/inquirer-file-selector.git"
